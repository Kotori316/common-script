{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchManagers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "teamreborn",
        "RebornCore",
        "TechReborn",
        "com.refinedmods:refinedstorage"
      ],
      "enabled": false
    },
    {
      "matchManagers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "^appeng:appliedenergistics2*"
      ],
      "registryUrls": [
        "https://modmaven.dev"
      ],
      "ignoreUnstable": false
    },
    {
      "matchManagers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "^com.kotori316:test-utility",
        "^com.kotori316:debug-utility"
      ],
      "registryUrls": [
        "https://maven.kotori316.com"
      ],
      "major": {
        "enabled": false
      },
      "minor": {
        "enabled": false
      }
    },
    {
      "matchManagers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "^com.kotori316:VersionCheckerMod"
      ],
      "registryUrls": [
        "https://maven.kotori316.com"
      ]
    }
  ]
}
